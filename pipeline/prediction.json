{
	"name": "prediction",
	"properties": {
		"activities": [
			{
				"name": "prediction",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "delete from predictions;",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "delivery",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "delivery_n",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "delivery_item",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "item",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "fail",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "fail",
									"type": "Double",
									"physicalType": "float"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "prediction",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sql_prediction",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}